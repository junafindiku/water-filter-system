<script>
async function test () {
    const data = {
		"slotStartDateTime": '2012-11-04 08:30',
        "slotEndDateTime": '2012-11-04 10:00'
	};
    await fetch("/scheduleSlot",
        { method: "POST",
        headers: { "Content-Type": "application/json" },
        body: JSON.stringify(data)
    })
    .catch(err => console.log("Error while posting task to server", err))
    .then((response) => response.json())
        .then((rs) => console.log(rs));
}
</script>
<a href="javascript:test();">Test API</a>